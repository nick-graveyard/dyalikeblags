<!DOCTYPE html>
<html>
<head>
  <title>Transient Design</title>


  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <%= stylesheet_link_tag    'bootstrap/css/bootstrap.css', media: 'all' %>
  <%= javascript_include_tag 'bootstrap/js/bootstrap.js' %>
  <%= stylesheet_link_tag    'custom.css', media: 'all' %>

 
</head>

<body>
<%# Navbar %>
<%= render "dyalikeblags/dashboard/partials/navbar" %>



<div class="container-fluid">
 <div class="row">
    <div class="col-md-2 sidebar">

    	<%# Sidebar %>
			<%= render "dyalikeblags/dashboard/partials/sidebar" %>

		</div>

		
		 <div class="col-md-10 col-md-offset-2 main">
	
					 	<div id="notice" style="margin:0px; padding:0px;  color:green"><%= notice %></div>
			
			<%# Content %>
			<%= yield %>
		</div>

</div>
</div>



</body>
</html>
